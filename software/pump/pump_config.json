{
  "io": {
      "port": "/dev/ttyUSB0",
      "baudrate": 9600,
      "timeout": 1
  },
  "default":{
      "micro_step_mode": 2,
      "initialize_valve_position": "E",
      "top_velocity": 6000
  },
  "groups": {
      "oils": ["dep","octanoic","octanol","pentanol"]
  },
  "pumps": {
      "acetone_oil": {
        "switch": "0",
        "volume": 5
      },
      "waste_oil": {
        "switch": "1",
        "volume": 12.5
      },
      "acetone_dish": {
        "switch": "2",
        "volume": 5,
        "top_velocity": 12000
      },
      "waste_dish": {
        "switch": "3",
        "volume": 12.5
      },
      "water_dish": {
        "switch": "4",
        "volume": 5,
        "top_velocity": 12000
      },
      "surfactant": {
        "switch": "5",
        "volume": 5
      },
      "dep": {
        "switch": "6",
        "volume": 1
      },
      "octanoic": {
        "switch": "7",
        "volume": 1
      },
      "octanol": {
        "switch": "8",
        "initialize_valve_position": "I",
        "volume": 1
      },
      "pentanol": {
        "switch": "9",
        "initialize_valve_position": "I",
        "volume": 1
      }
  }
}
